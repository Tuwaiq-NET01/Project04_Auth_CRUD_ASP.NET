@model ReservationsModel
@using Microsoft.AspNetCore.Identity
@inject UserManager<IdentityUser> UserManager
@Html.Partial("_NavParkingPartial")
<br/><br/>
     <div style="padding-left: 150px; ">
         <div style="align-content: center;box-shadow:5px 5px #000000; text-align: center; padding-left: 200px; background-color: rgb(0,0,0,0.6); width:1000px ">

             <form asp-action="Create" method="post" class="form-horizontal">
                 <div class="form-group w-75">
                     <label style="font-size:25px; color:white; text-shadow: 5px 5px #000000;" asp-for="Id"><b>Id Reservation</b></label>
                     <input style="text-align:center" class="form-control" asp-for="Id" />
                 </div>
                 <div class="form-group w-75">
                     <label style="font-size: 25px; color: white; text-shadow: 5px 5px #000000;" asp-for="Date"><b>Date</b></label>
                     <input style="text-align:center" class="form-control" asp-for="Date" />
                 </div>
                 <div class="form-group w-75">
                     <label style="font-size: 25px; color: white; text-shadow: 5px 5px #000000;" asp-for="ParkingId"><b>Parking Id</b> </label>
                     <input style="text-align:center" class="form-control" asp-for="ParkingId" />
                 </div>
                 <div class="form-group w-75">
                     <input type="submit" class="btn btn-warning" value="Submit" />

                 </div>
             </form>
             @if (ViewBag.Message != null)
             {
                 <script type="text/javascript">
                    window.onload = function () {
                        alert("@ViewBag.Message")
                    };
                 </script>
             }
         </div>
     </div>